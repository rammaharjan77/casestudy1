<script>
    $("#startTour").hide();
//    $(".open-notifications").hide();
    $(document).ready(function (e) {

        /*android, ios, blackberry, etc issues start*/
        if (/Android|webOS|iPhone|iPad|iPod|BlackBerry/i.test(navigator.userAgent)) {

        }
        /*android, ios, blackberry, etc issues start*/

        $('.search-icon-nav').click(function (e) {
            $('.course-search-box-nav').slideToggle();
        });
    });
     
     if ($(window).width() > 776) {
        //$('body').css({"margin-top": "0"});
        $(window).scroll(function () {
            var scrollTop = $(window).scrollTop();
             if ($(window).scrollTop() == 0) {
               $('.navbar-homepage').css({"position": "fixed", "background": "#222", "width": "100%", "z-index": "0"});
               $('.searchox-menu, .course-search-box-nav').show();
               $('.typeahead__button').css({"display":"block"});
            }
            else if (scrollTop >400) {
               // console.log("2");
                $('.navbar-homepage').css({"position": "fixed", "background-color": "#222", "width": "100%", "z-index": "11"});
                $('.searchox-menu, .course-search-box-nav').show();
                $('header').css({"position": "static"});
                $('.typeahead__button').css({"display":"none"});


            } else {
               // console.log(3);
                $('.navbar-homepage').css({"position": "fixed", "background": "#222", "width": "100%", "z-index": "0"});
                $('.searchox-menu, .course-search-box-nav').hide();
                $('header').css({"position": "relative"});
                $('.typeahead__button').css({"display":"block"});
                //$('body').css({"margin-top": "-21px"});
            }

        });
    } else {
        $('body').css({"margin-top": "0"});
       $('.navbar-homepage').css({"position": "fixed", "background": "#222", "width": "100%", "z-index": "0"});
    }
</script>

