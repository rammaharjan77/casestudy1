            function prepareTrendingCourses() {
                var data = JSON.parse(trendingCourseList);
                trendingCourseList = data.reduce(function(newData, currentValue, currentIndex) {
                    if(newData) {
                        newData.push({
                            "short_name": currentValue.Course.short_name,
                            "slug": currentValue.Course.slug,
                            "id": currentValue.TrendingCourse.course_id,
                            "title": currentValue.TrendingCourse.title,
                            "class": 'trending-course',
                            "dataId": currentIndex+1
                        })
                        return newData
                    }
                }, [])
            }
            prepareTrendingCourses()


